generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}



model Field{
    image1 String
    image2 String
    fieldCode String @id 
    name String
    location String
    size Float
    crop Crop[]
    log Log[]
    Equipment Equipment[]
}

model Crop{
    image String 
    cropCode String @id 
    name String
    scientificName String
    sesson String
    category String
    field String
    log Log[]
    fields Field @relation(fields: [field],references: [fieldCode])
    
}



